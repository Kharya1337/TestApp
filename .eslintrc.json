{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "commonjs": true
  },
  "rules": {
    "newline-before-return": 1,
    "newline-after-var": [
      1,
      "always"
    ],
    "no-use-before-define": 0,
    "object-curly-newline": 0,
    "react/jsx-filename-extension": 0,
    "comma-dangle": 0,
    "import/no-unresolved": 0,
    "react/jsx-one-expression-per-line": 0
  },
  "globals": {
    "fetch": false
  }
}